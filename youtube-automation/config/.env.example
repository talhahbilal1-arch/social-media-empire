# YouTube Shorts Automation - Environment Variables
# Copy this file to .env and fill in your values

# ElevenLabs - Voice Generation
ELEVENLABS_API_KEY=your_key_here
ELEVENLABS_VOICE_ID=your_cloned_voice_id

# D-ID - Avatar Video Generation
DID_API_KEY=your_key_here
DID_PRESENTER_ID=your_avatar_id
DID_AVATAR_IMAGE_URL=https://your-hosted-image-url.jpg

# Late.dev - YouTube Upload & Scheduling
LATE_API_KEY=your_key_here
LATE_YOUTUBE_ACCOUNT_ID=your_channel_id

# Google Sheets - Script Storage
GOOGLE_SHEETS_ID=your_sheet_id

# Notifications (Optional)
SLACK_WEBHOOK_URL=optional
ALERT_EMAIL=optional@email.com
